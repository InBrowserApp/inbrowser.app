{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "./**/src/**/*",
    "./**/src/**/*.vue"
  ],
  "exclude": [
    "./**/src/**/__tests__/*",
    "node_modules",
    "./**/node_modules"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "lib": [
      "ES2021",
      "ES2022.Intl"
    ]
  }
}